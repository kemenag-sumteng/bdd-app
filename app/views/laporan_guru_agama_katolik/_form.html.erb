<%= bootstrap_form_for(@laporan_guru_agama_katolik, local: true) do |form| %>
  <div class="card-body">
    <div class="field">
      <%= form.text_field :judul %>
    </div>
    <div class="field">
      <%= form.text_field :tautan %>
    </div>
    <div class="field">
      <%= form.text_field :nama_guru %>
    </div>
    <div class="actions">
      <%= form.submit "Membuat Laporan Guru Agama Katolik", class: "btn btn-success" %>
    </div>
  </div>
<% end %>