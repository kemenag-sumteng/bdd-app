<div class="card">
  <h1 class="card-header">Laporan Penyuluh Agama Katolik</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Bulan</th>
      <th scope="col">Tahun</th>
      <th scope="col">Tautan</th>
      <th scope="col">Nama penyuluh</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laporan_penyuluh_agama_katolik.each do |laporan_penyuluh_agama_katolik| %>
      <tr scope="row">
        <td><%= laporan_penyuluh_agama_katolik.bulan %></td>
        <td><%= laporan_penyuluh_agama_katolik.tahun %></td>
        <td><%= laporan_penyuluh_agama_katolik.tautan %></td>
        <td><%= laporan_penyuluh_agama_katolik.nama_penyuluh %></td>
        <td><%= link_to fa_icon("eye"), laporan_penyuluh_agama_katolik %></td>
        <td><%= link_to fa_icon("pencil"), edit_laporan_penyuluh_agama_katolik_path(laporan_penyuluh_agama_katolik) %></td>
        <td><%= link_to fa_icon("trash"), laporan_penyuluh_agama_katolik, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Laporan Penyuluh Agama Katolik', new_laporan_penyuluh_agama_katolik_path %>
