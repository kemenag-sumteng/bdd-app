<p id="notice"><%= notice %></p>

<h1>Laporan Penyuluh Agama Katolik</h1>

<table>
  <thead>
    <tr>
      <th>Bulan</th>
      <th>Tahun</th>
      <th>Tautan</th>
      <th>Nama penyuluh</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laporan_penyuluh_agama_katolik.each do |laporan_penyuluh_agama_katolik| %>
      <tr>
        <td><%= laporan_penyuluh_agama_katolik.bulan %></td>
        <td><%= laporan_penyuluh_agama_katolik.tahun %></td>
        <td><%= laporan_penyuluh_agama_katolik.tautan %></td>
        <td><%= laporan_penyuluh_agama_katolik.nama_penyuluh %></td>
        <td><%= link_to 'Show', laporan_penyuluh_agama_katolik %></td>
        <td><%= link_to 'Edit', edit_laporan_penyuluh_agama_katolik_path(laporan_penyuluh_agama_katolik) %></td>
        <td><%= link_to 'Destroy', laporan_penyuluh_agama_katolik, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

