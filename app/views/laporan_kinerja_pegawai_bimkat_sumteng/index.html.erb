<div class="card">
  <h1 class="card-header">Laporan Kinerja Pegawai Bimkat Sumteng</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Judul</th>
      <th scope="col">Tautan</th>
      <th scope="col">Nama pegawai</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @laporan_kinerja_pegawai_bimkat_sumteng.each do |laporan_kinerja_pegawai_bimkat_sumteng| %>
      <tr scope="row">
        <td><%= laporan_kinerja_pegawai_bimkat_sumteng.judul %></td>
        <td><%= laporan_kinerja_pegawai_bimkat_sumteng.tautan %></td>
        <td><%= laporan_kinerja_pegawai_bimkat_sumteng.nama_pengawai %></td>
        <td><%= link_to fa_icon("eye"), laporan_kinerja_pegawai_bimkat_sumteng %></td>
        <td><%= link_to fa_icon("pencil"), edit_laporan_kinerja_pegawai_bimkat_sumteng_path(laporan_kinerja_pegawai_bimkat_sumteng) %></td>
        <td><%= link_to fa_icon("trash"), laporan_kinerja_pegawai_bimkat_sumteng, method: :delete, data: { confirm: 'Anda yakin?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Membuat Laporan Kinerja Pegawai Bimkat Sumteng', new_laporan_kinerja_pegawai_bimkat_sumteng_path, :class => "btn btn-primary" %>
