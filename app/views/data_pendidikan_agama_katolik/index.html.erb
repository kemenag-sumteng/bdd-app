<div class="card">
  <h1 class="card-header">Data Pendidikan Agama Katolik</h1>
  <div class="card-body">
    <p class="card-text">Dibawah ini dapat Anda temukan Data Pendidikan Agama Katolik.</p>
  </div>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Judul</th>
      <th scope="col">Tautan</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <% @data_pendidikan_agama_katolik.each do |data_pendidikan_agama_katolik| %>
    <tr scope="row">
      <td><%= data_pendidikan_agama_katolik.judul %></td>
      <td><%= data_pendidikan_agama_katolik.tautan %></td>
      <td><%= link_to 'Show', data_pendidikan_agama_katolik %></td>
      <td><%= link_to 'Edit', edit_data_pendidikan_agama_katolik_path(data_pendidikan_agama_katolik) %></td>
      <td><%= link_to 'Destroy', data_pendidikan_agama_katolik, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Pendidikan Agama Katolik', new_data_pendidikan_agama_katolik_path %>
