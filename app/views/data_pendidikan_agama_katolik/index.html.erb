<div class="card">
  <h1 class="card-header">Data Pendidikan Agama Katolik</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Judul</th>
      <th scope="col">Tautan</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <% @data_pendidikan_agama_katolik.each do |data_pendidikan_agama_katolik| %>
    <tr scope="row">
      <td><%= data_pendidikan_agama_katolik.judul %></td>
      <td><%= truncate(data_pendidikan_agama_katolik.tautan) %></td>
      <td><%= link_to fa_icon("eye"), data_pendidikan_agama_katolik %></td>
      <td><%= link_to fa_icon("pencil"), edit_data_pendidikan_agama_katolik_path(data_pendidikan_agama_katolik) %></td>
      <td><%= link_to fa_icon("trash"), data_pendidikan_agama_katolik, method: :delete, data: { confirm: 'Anda yakin?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
<% if can? :create, @data_pendidikan_agama_katolik %>
  <%= link_to "Membuat Data Pendidikan Agama  Katolik", new_data_pendidikan_agama_katolik_path, :class => "btn btn-primary" %>
<% end %>