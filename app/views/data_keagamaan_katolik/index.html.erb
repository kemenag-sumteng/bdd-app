<%= render "partial/navigation" %>
<div class="card">
  <p id="notice"><%= notice %></p>
  <h1 class="card-header">Data Keagamaan Katolik</h1>
  <div class="card-body">
    <p class="card-text">Dibawah ini dapat Anda temukan Data Kegamaan Katolik.</p>
  </div>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Judul</th>
      <th scope="col">Tautan</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <% @data_keagamaan_katolik.each do |data_keagamaan_katolik| %>
    <tr scope="row">
      <td><%= data_keagamaan_katolik.judul %></td>
      <td><%= data_keagamaan_katolik.tautan %></td>
      <td><%= link_to 'Show', data_keagamaan_katolik %></td>
      <td><%= link_to 'Edit', edit_data_keagamaan_katolik_path(data_keagamaan_katolik) %></td>
      <td><%= link_to 'Destroy', data_keagamaan_katolik, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Keagamaan Katolik', new_data_keagamaan_katolik_path %>
